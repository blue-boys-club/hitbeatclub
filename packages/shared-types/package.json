{
	"name": "@hitbeatclub/shared-types",
	"version": "0.1.0",
	"description": "Shared types and schemas for HitBeatClub frontend and backend",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./user": {
			"types": "./dist/user/index.d.ts",
			"import": "./dist/user/index.mjs",
			"require": "./dist/user/index.js"
		},
		"./artist": {
			"types": "./dist/artist/index.d.ts",
			"import": "./dist/artist/index.mjs",
			"require": "./dist/artist/index.js"
		},
		"./product": {
			"types": "./dist/product/index.d.ts",
			"import": "./dist/product/index.mjs",
			"require": "./dist/product/index.js"
		},
		"./order": {
			"types": "./dist/order/index.d.ts",
			"import": "./dist/order/index.mjs",
			"require": "./dist/order/index.js"
		},
		"./auth": {
			"types": "./dist/auth/index.d.ts",
			"import": "./dist/auth/index.mjs",
			"require": "./dist/auth/index.js"
		},
		"./common": {
			"types": "./dist/common/index.d.ts",
			"import": "./dist/common/index.mjs",
			"require": "./dist/common/index.js"
		},
		"./subscribe": {
			"types": "./dist/subscribe/index.d.ts",
			"import": "./dist/subscribe/index.mjs",
			"require": "./dist/subscribe/index.js"
		},
		"./file": {
			"types": "./dist/file/index.d.ts",
			"import": "./dist/file/index.mjs",
			"require": "./dist/file/index.js"
		},
		"./settlement": {
			"types": "./dist/settlement/index.d.ts",
			"import": "./dist/settlement/index.mjs",
			"require": "./dist/settlement/index.js"
		},
		"./tag": {
			"types": "./dist/tag/index.d.ts",
			"import": "./dist/tag/index.mjs",
			"require": "./dist/tag/index.js"
		},
		"./genre": {
			"types": "./dist/genre/index.d.ts",
			"import": "./dist/genre/index.mjs",
			"require": "./dist/genre/index.js"
		},
		"./email": {
			"types": "./dist/email/index.d.ts",
			"import": "./dist/email/index.mjs",
			"require": "./dist/email/index.js"
		},
		"./cart": {
			"types": "./dist/cart/index.d.ts",
			"import": "./dist/cart/index.mjs",
			"require": "./dist/cart/index.js"
		},
		"./coupon": {
			"types": "./dist/coupon/index.d.ts",
			"import": "./dist/coupon/index.mjs",
			"require": "./dist/coupon/index.js"
		},
		"./notice": {
			"types": "./dist/notice/index.d.ts",
			"import": "./dist/notice/index.mjs",
			"require": "./dist/notice/index.js"
		},
		"./payment": {
			"types": "./dist/payment/index.d.ts",
			"import": "./dist/payment/index.mjs",
			"require": "./dist/payment/index.js"
		}
	},
	"files": [
		"dist/**"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rm -rf dist",
		"type-check": "tsc --noEmit",
		"lint": "eslint src --ext .ts,.tsx",
		"test": "jest"
	},
	"dependencies": {
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@hitbeatclub/eslint-config": "workspace:*",
		"@hitbeatclub/typescript-config": "workspace:*",
		"@types/node": "^20.10.0",
		"eslint": "^8.55.0",
		"jest": "^29.7.0",
		"tsup": "^8.0.1",
		"typescript": "^5.3.0"
	},
	"publishConfig": {
		"access": "restricted"
	}
}
